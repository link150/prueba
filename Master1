def python_function(numeros1,numeros2, multiplo):

 suma = 0

 for i in numeros1:

      if i % multiplo == 0:

         suma+=i

 for i in numeros2:
      if i % multiplo == 0:

         suma+=i

 return suma

numeros1 = [3,5,4,3,6,8]

numeros2 = [2,3,5,8,2,4]

multiplo = 3


print(python_function(numeros1,numeros2,multiplo))